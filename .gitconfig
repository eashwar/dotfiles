[user]
	email = eashwar@umich.edu
	name = Eashwar Mohan
[credential]
	helper = /mnt/c/Program\\ Files/Git/mingw64/libexec/git-core/git-credential-wincred.exe
[core]
	editor = vim
	autocrlf = input
	safecrlf = false
[push]
	default = matching
[alias]
	dog = log --all --decorate --oneline --graph
	ssh = remote set-url origin
	forcepull = "!f() { git reset --hard HEAD; git pull; }; f"
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
