[user]
	email = eashwar@umich.edu
	name = Eashwar Mohan
[core]
	editor = vim
	autocrlf = input
	safecrlf = false
[push]
	default = current
[alias]
    st = status
	dog = log --all --decorate --oneline --graph
	ssh = remote set-url origin
	forcepull = "!f() { git reset --hard HEAD; git pull; }; f"
	last = log -1 HEAD
[credential]
	helper = store
