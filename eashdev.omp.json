{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#3d566c",
                    "foreground": "#d3f5ff",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
                    "type": "os"
                },
                {
                    "background": "#ffe9aa",
                    "foreground": "#100e23",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \uf0e7 ",
                    "type": "root"
                },
                {
                    "background": "#318ba2",
                    "foreground": "#d3f5ff",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " {{ .UserName }} ",
                    "type": "session"
                },
                {
                    "background": "#2ec3c0",
                    "foreground": "#d3f5ff",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "folder_icon": "\uf115",
                        "folder_separator_icon": " \ue0b1 ",
                        "style": "folder",
                        "max_depth": 2
                    },
                    "style": "powerline",
                    "template": " {{ path .Path .Location }} ",
                    "type": "path"
                },
                {
                    "background": "#e59d23",
                    "foreground": "#3d566c",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "background": "#6f889f",
                    "foreground": "#d3f5ff",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
                    "type": "python"
                },
                {
                    "background": "#7bfac4",
                    "foreground": "#3d566c",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "background": "#ff8080",
                    "foreground": "#d3f5ff",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \ue20f ",
                    "type": "status"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#3d566c",
                    "style": "plain",
                    "template": "\u276f ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}